global DR_GROUP_ID
global drg
try:
    if DR_GROUP_ID >= 0:
        DR_GROUP_ID += 1
except NameError:
    DR_GROUP_ID = 0
    drg = []

drg.append(trick.DRAscii("rt_record"))
drg[DR_GROUP_ID].set_freq(trick.DR_Always)
drg[DR_GROUP_ID].set_cycle(0.001)
drg[DR_GROUP_ID].set_single_prec_only(False)
drg[DR_GROUP_ID].add_variable("trick_real_time.rt_sync.enable_flag")
drg[DR_GROUP_ID].add_variable("trick_real_time.itimer.enabled")
drg[DR_GROUP_ID].add_variable("trick_real_time.itimer.active")
drg[DR_GROUP_ID].add_variable("trick_real_time.rt_sync.disable_flag")
drg[DR_GROUP_ID].add_variable("trick_real_time.rt_sync.active")
drg[DR_GROUP_ID].add_variable("trick_real_time.rt_sync.freeze_frame")
drg[DR_GROUP_ID].add_variable("trick_real_time.rt_sync.freeze_time_tics")
drg[DR_GROUP_ID].add_variable("trick_real_time.rt_sync.frame_overrun_cnt")
drg[DR_GROUP_ID].add_variable("trick_real_time.rt_sync.total_overrun")
drg[DR_GROUP_ID].add_variable("trick_real_time.rt_sync.rt_max_overrun_cnt")
drg[DR_GROUP_ID].add_variable("trick_real_time.rt_sync.rt_max_overrun_time")
