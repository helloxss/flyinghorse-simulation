drg[DR_GROUP_ID].add_variable("trick_real_time.rt_sync.rt_clock[0].sim_tic_ratio")
drg[DR_GROUP_ID].add_variable("trick_real_time.rt_sync.rt_clock[0].rt_clock_ratio")
trick.add_data_record_group(drg[DR_GROUP_ID], trick.DR_Buffer)
drg[DR_GROUP_ID].enable()
