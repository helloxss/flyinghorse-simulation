class GPS_SAT_SimObject : public Trick::SimObject {
    public:
        GPS_Satellites sats;

        GPS_SAT_SimObject() {
            ("default_data") sats.default_data();
            ("initialization") sats.initialize();
            (0.001, "scheduled") sats.propagate_sv();
        }
};

class Rocket_SimObject : public Trick::SimObject {
    public:
        GPS_Satellites* gps_sats;

        Environment env;
        Newton newton;
        _Euler_ euler;

        Kinematics kinematics;
        Forces forces;

        AeroDynamics aerodynamics;
        Propulsion propulsion;

        GPS_Receiver gpsr;

        TVC tvc;
        RCS rcs;

        time_management time;

        Rocket_SimObject() 
            :   newton      ( kinematics , euler        , env          , propulsion   , forces )      ,
                kinematics  ( newton     , env          , euler )      ,
                euler       ( kinematics , propulsion   , forces )     ,
                gpsr        ( newton     , euler        , *gps_sats )        ,
                env         ( newton     , aerodynamics , kinematics , time ) ,
                propulsion  ( env )      ,
                aerodynamics( kinematics , env          , propulsion   , euler        , newton        , tvc ) ,
                tvc         ( env        , kinematics   , propulsion ) ,
                rcs         ( propulsion ) ,
                forces      ( env        , propulsion   , rcs          , aerodynamics , tvc ) ,
                time        ()
            {

            ("initialization") kinematics.initialize();
            ("initialization") newton.initialize();
            ("initialization") env.initialize();
            ("initialization") aerodynamics.initialize();
            ("initialization") euler.initialize();
            ("initialization") gpsr.initialize(0.001);
            ("initialization") tvc.initialize();
            ("initialization") rcs.initialize();
            ("initialization") propulsion.initialize();
            ("initialization") forces.initialize();

            P1 (0.001, "scheduled") time.dm_time();
            //P1 (0.001, "scheduled") gpsr.update_markov(0.001);
            P1 (0.001, "scheduled") kinematics.propagate(0.001);
	        P1 (0.001, "scheduled") env.propagate(0.001);
            P1 (0.001, "scheduled") propulsion.propagate(0.001);
            P1 (0.001, "scheduled") aerodynamics.calculate_aero(0.001);
            // P1 (0.1, "scheduled") gpsr.get_quadriga();
            // P1 (0.1, "scheduled") gpsr.filter_extrapolation(0.1);
            // P1 (0.1, "scheduled") gpsr.measure();

            P3 (0.001, "scheduled") tvc.actuate(0.001);
            P3 (0.001, "scheduled") rcs.rcs_schmitt_thrust();
            P3 (0.001, "scheduled") forces.collect_forces_and_propagate();
            P3 (0.001, "scheduled") newton.propagate(0.001);
            P3 (0.001, "scheduled") euler.propagate(0.001);

            (0.001, "logging") kinematics.update_diagnostic_attributes(0.001);
            (0.001, "logging") env.update_diagnostic_attributes(0.001);
            (0.001, "logging") newton.update_diagnostic_attributes(0.001);
            (0.001, "logging") euler.update_diagnostic_attributes(0.001);

        }
};

GPS_SAT_SimObject gps_sats;
Rocket_SimObject rkt;

class FlightComputer_SimObject : public Trick::SimObject {
    public:
        Rocket_SimObject *rkt;

        INS ins;

        Control control;
        Guidance guidance;

        RCS_FC rcs_fc;

        void link(){
            control.grab_mprop = LINK(rkt->propulsion, get_mprop);

            control.grab_dvbe  = LINK(rkt->newton, get_dvbe);

            control.grab_gymax = LINK(rkt->aerodynamics, get_gymax);
            control.grab_dyb   = LINK(rkt->aerodynamics, get_dyb);
            control.grab_dnb   = LINK(rkt->aerodynamics, get_dnb);
            control.grab_dnr   = LINK(rkt->aerodynamics, get_dnr);
            control.grab_dndr  = LINK(rkt->aerodynamics, get_dndr);
            control.grab_dla   = LINK(rkt->aerodynamics, get_dla);
            control.grab_dma   = LINK(rkt->aerodynamics, get_dma);
            control.grab_dmq   = LINK(rkt->aerodynamics, get_dmq);
            control.grab_dmde  = LINK(rkt->aerodynamics, get_dmde);
            control.grab_dnd   = LINK(rkt->aerodynamics, get_dnd);
            control.grab_dlde  = LINK(rkt->aerodynamics, get_dlde);

            control.grab_pdynmc = LINK(rkt->env, get_pdynmc);

            control.grab_dvbec = LINK(ins, get_dvbec);
            control.grab_thtvdcx = LINK(ins, get_thtvdcx);
            control.grab_thtbdcx = LINK(ins, get_thtbdcx);

            control.grab_qqcx = LINK(ins.get_gyro(), get_qqcx);
            control.grab_rrcx = LINK(ins.get_gyro(), get_rrcx);

            control.grab_FSPCB = LINK(ins.get_accelerometer(), get_computed_FSPB);


            ins.grab_SBII       = LINK(rkt->newton     , get_SBII);
            ins.grab_VBII       = LINK(rkt->newton     , get_VBII);
            ins.grab_dbi        = LINK(rkt->newton     , get_dbi);
            ins.grab_TBI        = LINK(rkt->kinematics , get_TBI);
            ins.grab_SXH        = LINK(rkt->gpsr       , get_SXH);
            ins.grab_VXH        = LINK(rkt->gpsr       , get_VXH);
            ins.grab_gps_update = LINK(rkt->gpsr       , get_gps_update);
            ins.clear_gps_flag  = LINK(rkt->gpsr       , clear_gps_flag);


            guidance.grab_mprop  = LINK(rkt->propulsion, get_mprop);
            guidance.grab_TBIC   = LINK(ins, get_TBIC);

            guidance.grab_dbi    = LINK(rkt->newton, get_dbi);
            guidance.grab_dvbi   = LINK(rkt->newton, get_dvbi);
            guidance.grab_thtvdx = LINK(rkt->newton, get_thtvdx);
            guidance.grab_fmassr = LINK(rkt->propulsion, get_fmassr);
            guidance.grab_VBIIC  = LINK(ins, get_VBIIC);
            guidance.grab_SBIIC  = LINK(ins, get_SBIIC);
            guidance.grab_FSPCB  = LINK(ins.get_accelerometer(), get_computed_FSPB);

            guidance.set_no_thrust  = LINK(rkt->propulsion, set_no_thrust);
            guidance.set_ltg_thrust = LINK(rkt->propulsion, set_ltg_thrust);


            rkt->tvc.grab_delrcx = LINK(control, get_delrcx);
            rkt->tvc.grab_delecx = LINK(control, get_delecx);

            rcs_fc.grab_qqcx = LINK(ins.get_gyro(), get_qqcx);
            rcs_fc.grab_ppcx = LINK(ins.get_gyro(), get_ppcx);
            rcs_fc.grab_rrcx = LINK(ins.get_gyro(), get_rrcx);

            rcs_fc.grab_alphacx = LINK(ins, get_alphacx);
            rcs_fc.grab_betacx = LINK(ins, get_betacx);

            rcs_fc.grab_thtbdcx = LINK(ins, get_thtbdcx);
            rcs_fc.grab_psibdcx = LINK(ins, get_psibdcx);
            rcs_fc.grab_phibdcx = LINK(ins, get_phibdcx);

            rcs_fc.grab_UTBC = LINK(guidance, get_UTBC);
            rcs_fc.grab_alphacomx = LINK(guidance, get_alphacomx);
            rcs_fc.grab_betacomx  = LINK(guidance, get_betacomx);


            rkt->forces.grab_rcs_isEnabled = LINK(rcs_fc, isEnabled);
            rkt->forces.grab_rcs_mode = LINK(rcs_fc, get_rcs_mode);


            rkt->rcs.grab_e_roll  = LINK(rcs_fc, get_e_roll);
            rkt->rcs.grab_e_pitch = LINK(rcs_fc, get_e_pitch);
            rkt->rcs.grab_e_yaw   = LINK(rcs_fc, get_e_yaw);


            rkt->gpsr.grab_SBIIC = LINK(ins, get_SBIIC);
            rkt->gpsr.grab_VBIIC = LINK(ins, get_VBIIC);
            rkt->gpsr.grab_WBICI = LINK(ins, get_WBICI);
        };

        FlightComputer_SimObject(){
            ("default_data") guidance.default_data();

            ("initialization") guidance.initialize();
            ("initialization") control.initialize();
            ("initialization") ins.initialize();
            ("initialization") rcs_fc.initialize();

            ("initialization") link();

            P2 (0.05, "scheduled") ins.get_gyro().propagate_error(0.05);
            P2 (0.05, "scheduled") ins.get_accelerometer().propagate_error(0.05);
            P2 (0.05, "scheduled") ins.update(0.05);
            // P2 (0.001, "scheduled") guidance.guidance(0.001);
            P2 (0.05, "scheduled") control.control(0.05);
            P2 (0.05, "scheduled") rcs_fc.actuate();
        }
};

FlightComputer_SimObject fc;

void create_connections() {
    rkt.gps_sats = &gps_sats.sats;

    fc.rkt = &rkt;
}

